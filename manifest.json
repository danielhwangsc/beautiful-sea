{
	"manifest_version": 2,
	"name": "Beautiful Sea",
	"version": "0.1",
	"description": "GitHub initial commit comment creator.",
	"browser_action": {
		"default_icon": "static/icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [
        {
            "matches": ["https://github.com/*/compare/*", "https://github.com/*/pull/*"],
            "js": ["jquery.js", "contentscript.js"]
        }
    ]
}